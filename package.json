    "@octokit/auth-app": "2.4.7",
    "@types/node": "13.13.9",
    "ansicolor": "^1.1.93",
    "config": "3.3.1",
    "cookie-parser": "1.4.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "graphql-request": "1.8.2",
    "http-errors": "1.7.3",
    "http-status-codes": "1.4.0",
    "inversify": "5.0.1",
    "inversify-express-utils": "6.3.2",
    "isomorphic-fetch": "2.2.1",
    "json-diff": "^0.5.4",
    "jsonschema": "1.2.6",
    "lodash.isequal": "^4.5.0",
    "minimatch": "3.0.4",
    "newrelic": "6.10.0",
    "osprey-middleware": "0.5.0",
    "pg": "7.18.2",
    "pg-hstore": "2.3.3",
    "postgraphile": "^4.7.0",
    "pug": "2.0.4",
    "reflect-metadata": "0.1.13",
