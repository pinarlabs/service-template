{
  "name": "service-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build-ts && db:migrate && npm run serve",
    "serve": "node dist/main.js",
    "build-ts": "npm run lint && tsc",
    "lint": "tsc --noEmit && eslint \"{src,test}/**/*.{js,ts}\" --quiet --fix",
    "lint:fix": "eslint src --fix",
    "db:migrate": "node db-migrate.js",
    "test": "npm run lint && NODE_ENV=test node ./tasks/jest-db.js --detectOpenHandles --forceExit",
    "test:db": "npm run lint && NODE_ENV=test node ./tasks/jest-db.js \"test/02-integration/.*.test.(js|ts)\" --detectOpenHandles --forceExit",
    "test:unit": "npm run lint && NODE_ENV=test jest \"test/01-unit/.*.test.(js|ts)\" --detectOpenHandles --forceExit",
    "test:only": "npm run lint && NODE_ENV=test jest \"test/.*.test.(js|ts)\" --detectOpenHandles --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinarlabs/service-template.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pinarlabs/service-template/issues"
  },
  "homepage": "https://github.com/pinarlabs/service-template#readme",
